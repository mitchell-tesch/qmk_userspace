CONVERT_TO=helios	# For use on the 0xCB Helios controller

LTO_ENABLE = yes	# Link Time Optimization

OLED_ENABLE = yes	# Enable OLED screens
WPM_ENABLE = yes 	# Enable word per minute counter
TAP_DANCE_ENABLE = yes	# Enable tap dance feature
TAP_CODE_DELAY = 100 	# Delay between tap and hold in ms
EXTRAKEY_ENABLE = yes   # Audio control and System control(+470)

MOUSEKEY_ENABLE = no    # Mouse keys(+1482)
MAGIC_ENABLE = no
GRAVE_ESC_ENABLE = no
SPACE_CADET_ENABLE = no
COMMAND_ENABLE = no

RGB_MATRIX_ENABLE = no		# Matrix LEDs
